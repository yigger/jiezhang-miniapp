.jz-pages__finance {
  background-color: #ececec;

  .jz-pages__finance-list__item {
    background-color: white;
    margin-bottom: 12PX
  }

  .jz-pages__finance__child-list {
    font-size: 14PX
  }

  .jz-pages__finance__child-total {
    padding: 8PX 16PX;
    border-bottom: 1px dashed #dcdcdc;
    font-size: 13PX;
    color: #898c8e;
  }

  .jz-pages__finance-header {
    background-color: #fdfd82
    padding: 16PX
  }


  .jz-pages__finance-list {
    .icon-image {
      width: 30PX;
      height: 30PX;
      image {
        width: 100%;
        height: 100%;
      }
    }
  }
}